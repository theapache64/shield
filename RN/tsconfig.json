{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "es6",
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": true,
    "target": "es6"
  },
  "exclude": [
    "node_modules"
  ]
}
